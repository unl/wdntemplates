{
  "presets": [
    ["@babel/preset-env",
      {
        "corejs": { "version": "3.15", "proposals": true },
        "useBuiltIns": "entry"
      }
    ]
  ],
  "plugins": [
    "@babel/plugin-proposal-class-properties",
    "@babel/plugin-proposal-object-rest-spread",
    "@babel/plugin-transform-classes"
  ]
}
